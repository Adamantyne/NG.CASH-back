{
  "name": "ng.cash-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run prisma:migrate-deploy && npm run prisma:generate && node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "test":"npm run prisma:test && npm run test-options dotenv -e .env.test jest -- -i -- app.test.ts",
    "test:local": "npm run prisma:test-local && npm run test-options dotenv -e .env.test.local jest -- -i -- app.test.ts",
    "test:unit": "npm run test-options jest -- -i --coverage --collectCoverageFrom=src/services/ -- unit.test.ts",
    "test:docker": "npm run test:compose -- up",
    "test:compose": "docker-compose -f docker-compose.test.yml --env-file .env.test run node_app npm run test",
    "test-options": "NODE_OPTIONS=--experimental-vm-modules",
    "prisma:generate": "prisma generate",
    "prisma:migrate-deploy": "prisma migrate deploy",
    "prisma:migrate-dev": "prisma migrate dev",
    "prisma:test": "dotenv -e .env.test prisma migrate dev --skip--seed",
    "prisma:test-local": "dotenv -e .env.test.local prisma migrate reset --skip--seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@prisma/client": "^4.6.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.6.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
